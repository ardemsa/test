    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "evaluate_classical.py",
            "console": "integratedTerminal",
            "args": "--pred=evaluation_examples/classical_test_gold.txt --out_file=all_eval_results.json"
        }
    ]
}